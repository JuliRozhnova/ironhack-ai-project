<div class="test">
  <img class="imgSelf" src="{{emotion.photo}}" alt="{{user.username}}">
</div>
<p>Your current mood: {{emotion.emotion}}</p>
<p>Your playlists:</p>

<div class="container">
  {{#each emotion.playlists}}
  <div>
    <div class="container-iframe">
      <iframe width="360" height="215" src="https://www.youtube.com/embed/videoseries?list={{itemId}}" frameborder="0"
        allow="autoplay; encrypted-media" allowfullscreen></iframe>
    </div>
    <h3>{{title}}</h3>
    {{#if description}}
    <p>{{description}}</p>
    {{/if}}
    <a href="https://www.youtube.com/channel/{{channelId}}" alt='{{title}}' target="_blank">GO TO
      CHANNEL</a>
  </div>
  {{/each}}
</div>